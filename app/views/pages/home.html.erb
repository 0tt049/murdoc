<div class="my-grid">

  <div data-controller="search-nodes">
    <div class='card card-parent' style="float: left;">

      <div id='card-text'>
        <div data-search-nodes-target="list">
        <%= render partial: "pages/children_list", locals: {nodes: @nodes} %>
        </div>
      </div>
      <div class='inner-search' id='inner-search'>
        <%= render "shared/searchbar" %>
      </div>

    </div>
  </div>
  <div>
    <%= render partial: "pages/home_content", locals: {node: @node, doc_node: @doc_node} %>
  </div>
</div>
